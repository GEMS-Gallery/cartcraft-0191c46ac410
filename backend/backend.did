type Item = 
 record {
   description: opt text;
   icon: text;
   id: text;
   name: text;
   price: opt float64;
 };
type Category = 
 record {
   icon: text;
   id: text;
   name: text;
 };
service : {
  getCategories: () -> (vec Category) query;
  getItemsByCategory: (text) -> (opt vec Item) query;
  initialize: () -> ();
}
